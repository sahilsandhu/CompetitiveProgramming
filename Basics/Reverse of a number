import java.util.*;
   
   public class Main{
   
   public static void main(String[] args) {
     // write your code here
    Scanner scn = new Scanner(System.in);
    int n = scn.nextInt();
    int count = 0;
    int temp = n;
    while(temp!=0)
    {
        count++;
        temp = temp/10;
    }
    temp = n;
    int ans = 0;
    int pow = 1;
    while(temp!=0)
    {
        int q = temp/10;
        int r = temp%10;
        temp = q;
      System.out.println(r);
    }
    
   }
    
   }
